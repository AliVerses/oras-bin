{
  "name": "oras-bin-wrapper",
  "version": "0.0.1-pre",
  "description": "Node.js wrapper for oras CLI binaries (cross-platform)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "postinstall": "node postinstall.js || node dist/postinstall.js",
    "test": "mocha -r ts-node/register test/**/*.ts"
  },
  "files": [
    "dist",
    "lib",
    "postinstall.js"
  ],
  "keywords": [
    "oras",
    "binary",
    "cli",
    "wrapper"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/tar": "^6.1.13",
    "@types/unzipper": "^0.10.11",
    "assert": "^2.0.0",
    "mocha": "^10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "tar": "^7.4.3",
    "unzipper": "^0.12.3"
  }
}
